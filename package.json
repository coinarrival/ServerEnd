{
  "name": "coinarrival-serverend",
  "version": "1.0.0",
  "description": "ServerEnd for project coinarrival",
  "main": "app.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha",
    "cover": "nyc npm run test && nyc report --reporter=text-lcov | coveralls",
    "start": "node ./bin/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coinarrival/ServerEnd.git"
  },
  "keywords": [
    "coinarrival",
    "serverend",
    "koa",
    "koa2"
  ],
  "author": "coinarrival",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coinarrival/ServerEnd/issues"
  },
  "homepage": "https://github.com/coinarrival/ServerEnd#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.7.0",
    "koa-body": "^4.1.0",
    "koa-jwt": "^3.5.1",
    "koa-router": "^7.4.0",
    "log4js": "^4.3.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.4",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "supertest": "^4.0.2"
  },
  "nyc": {
    "all": true,
    "exclude": [
      "*/controllers/*.js",
      "*/middleware/*.js",
      "*/config/*.js",
      "coverage/lcov-report/*.js",
      "*/utils/logger.js"
    ]
  }
}
